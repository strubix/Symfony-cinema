<?php

namespace MainBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * Abonnement
 */
class Abonnement
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var integer
     */
    private $idAbo;

    /**
     * @var string
     */
    private $nom;

    /**
     * @var string
     */
    private $resum;

    /**
     * @var integer
     */
    private $prix;

    /**
     * @var \DateTime
     */
    private $dureeAbo;


    /**
     * Get id
     *
     * @return integer 
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set idAbo
     *
     * @param integer $idAbo
     * @return Abonnement
     */
    public function setIdAbo($idAbo)
    {
        $this->idAbo = $idAbo;

        return $this;
    }

    /**
     * Get idAbo
     *
     * @return integer 
     */
    public function getIdAbo()
    {
        return $this->idAbo;
    }

    /**
     * Set nom
     *
     * @param string $nom
     * @return Abonnement
     */
    public function setNom($nom)
    {
        $this->nom = $nom;

        return $this;
    }

    /**
     * Get nom
     *
     * @return string 
     */
    public function getNom()
    {
        return $this->nom;
    }

    /**
     * Set resum
     *
     * @param string $resum
     * @return Abonnement
     */
    public function setResum($resum)
    {
        $this->resum = $resum;

        return $this;
    }

    /**
     * Get resum
     *
     * @return string 
     */
    public function getResum()
    {
        return $this->resum;
    }

    /**
     * Set prix
     *
     * @param integer $prix
     * @return Abonnement
     */
    public function setPrix($prix)
    {
        $this->prix = $prix;

        return $this;
    }

    /**
     * Get prix
     *
     * @return integer 
     */
    public function getPrix()
    {
        return $this->prix;
    }

    /**
     * Set dureeAbo
     *
     * @param \DateTime $dureeAbo
     * @return Abonnement
     */
    public function setDureeAbo($dureeAbo)
    {
        $this->dureeAbo = $dureeAbo;

        return $this;
    }

    /**
     * Get dureeAbo
     *
     * @return \DateTime 
     */
    public function getDureeAbo()
    {
        return $this->dureeAbo;
    }
}
